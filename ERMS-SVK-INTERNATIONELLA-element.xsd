<?xml version="1.0" encoding="UTF-8"?><!-- ERMS-SVK-INTERNATIONELLA-element version 1.1 2023-09-01 -->	<!-- Elementet partnerData har lagts till /Henrik Vitalis -->	<!-- ERMS-SVK-INTERNATIONELLA-element version 1.0 2023-02-26 --><xs:schema	xmlns:xs="http://www.w3.org/2001/XMLSchema"	xmlns="https://xml.svenskakyrkan.se/ERMS-SVK-INTERNATIONELLA-element"	xmlns:erms="https://xml.svenskakyrkan.se/ERMS-SVK-INTERNATIONELLA"	targetNamespace="https://xml.svenskakyrkan.se/ERMS-SVK-INTERNATIONELLA-element" 	elementFormDefault="qualified" attributeFormDefault="unqualified">	<xs:import namespace="https://xml.svenskakyrkan.se/ERMS-SVK-INTERNATIONELLA" schemaLocation="ERMS-SVK-INTERNATIONELLA.xsd"/>		<xs:element name="svkErmsInternationella">		<xs:complexType>			<xs:sequence>				<xs:element name="agreement" type="agreementType" minOccurs="0"/>				<xs:element name="approval"  type="approvalType" minOccurs="0"/>				<xs:element name="decisionLogs" type="decisionLogsType" minOccurs="0"/>				<xs:element name="funding" type="fundingType" minOccurs="0"/>				<xs:element name="bridgeingOutcomes" type="bridgeingOutcomesType" minOccurs="0"/>				<xs:element name="risks" type="risksType" minOccurs="0"/>				<xs:element name="geography" type="geographyType" minOccurs="0"/>				<xs:element name="monitorings" type="monitoringsType" minOccurs="0"/>				<xs:element name="partnerDatat" type="partnerDataType" minOccurs="0"/>			</xs:sequence>			<xs:attribute name="schemaVersion" type="xs:decimal" fixed="1.1" />		</xs:complexType>	</xs:element>		<xs:complexType name="agreementType">		<xs:sequence>			<xs:element name="agreementTitle" type="xs:string"/>			<xs:element name="typeOfAgreement" type="xs:string"/>			<xs:element ref="erms:agent" maxOccurs="unbounded"/>			<xs:element ref="erms:dates"/>			<xs:element name="aggrementDescription" type="xs:string"/>			<xs:element name="signatures" type="xs:string"/>		</xs:sequence>	</xs:complexType>		<xs:complexType name="approvalType">		<xs:annotation>			<xs:documentation>Describes approval/rejection of idea</xs:documentation>		</xs:annotation>		<xs:sequence>			<xs:element ref="erms:agent" minOccurs="0" maxOccurs="unbounded"/>			<xs:element ref="erms:dates" minOccurs="0"/>			<xs:element name="decision" type="xs:string"/>			<xs:element name="note" type="xs:string"/>			<xs:element name="status" type="xs:string"/>		</xs:sequence>	</xs:complexType>		<xs:complexType name="decisionLogsType">		<xs:sequence>			<xs:element name="decisionLog" maxOccurs="unbounded">				<xs:complexType>					<xs:sequence>						<xs:element name="decisionDescription" type="xs:string"/>						<xs:element ref="erms:dates"/>						<xs:element name="typeOfDecision" type="xs:string"/>						<xs:element ref="erms:agent" maxOccurs="unbounded"/>					</xs:sequence>				</xs:complexType>			</xs:element>		</xs:sequence>	</xs:complexType>		<xs:complexType name="fundingType">		<xs:annotation>			<xs:documentation>describe funding</xs:documentation>		</xs:annotation>		<xs:sequence>			<xs:element ref="erms:extraId"/>			<xs:element name="focusArea" type="xs:string"/>		</xs:sequence>	</xs:complexType>		<xs:complexType name="bridgeingOutcomesType">		<xs:annotation>			<xs:documentation>Describe goals and how work with them is implemented</xs:documentation>		</xs:annotation>		<xs:sequence>			<xs:element name="bridgeingOutcome" maxOccurs="unbounded">				<xs:complexType>					<xs:sequence>						<xs:element name="title" type="xs:string"/>						<xs:element name="description" type="xs:string"/>						<xs:element name="change" type="xs:string"/>						<xs:element ref="erms:note"/>						<xs:element ref="erms:agent"/>						<xs:element name="monitoring" type="xs:string"/>						<xs:element ref="erms:relation" maxOccurs="unbounded"/>						<xs:element name="progressMarkers">							<xs:complexType>								<xs:sequence>									<xs:element name="progressMarker" maxOccurs="unbounded">										<xs:complexType>											<xs:sequence>												<xs:element name="title" type="xs:string"/>												<xs:element name="expectationLevel" type="xs:string"/>												<xs:element name="status" type="xs:string"/>												<xs:element name="note" type="xs:string"/>												<xs:element name="goalAchievement" type="xs:string"/>											</xs:sequence>										</xs:complexType>									</xs:element>								</xs:sequence>							</xs:complexType>						</xs:element>						<xs:element name="outcomes">							<xs:complexType>								<xs:sequence>									<xs:element name="outcome" maxOccurs="unbounded">										<xs:complexType>											<xs:sequence>												<xs:element ref="erms:extraId"/>												<xs:element name="year" type="xs:integer"/>												<xs:element name="importance" type="xs:string"/>												<xs:element name="contribution" type="xs:string"/>												<xs:element name="thematicPriorities" type="xs:string"/>												<xs:element name="relatedToSDG" type="xs:string"/>												<xs:element name="substantationOrVerification" type="xs:string"/>												<xs:element ref="erms:agent"/>												<xs:element name="description" type="xs:string"/>											</xs:sequence>										</xs:complexType>									</xs:element>								</xs:sequence>							</xs:complexType>						</xs:element>					</xs:sequence>					<xs:attribute name="typeOfBridgeingOutcome" type="xs:string"/>				</xs:complexType>			</xs:element>		</xs:sequence>	</xs:complexType>		<xs:complexType name="risksType">		<xs:annotation>			<xs:documentation>Describes riskassesment for projects or programmes</xs:documentation>		</xs:annotation>		<xs:sequence>			<xs:element name="risk" maxOccurs="unbounded">				<xs:complexType>					<xs:sequence>						<xs:element name="typeOfRisk" type="xs:string"/>						<xs:element name="description" type="xs:string"/>						<xs:element name="source" type="xs:string"/>						<xs:element name="consequences" type="xs:string"/>						<xs:element name="likelihood" type="xs:string"/>						<xs:element name="impact" type="xs:string"/>						<xs:element name="riskRating" type="xs:string"/>						<xs:element ref="erms:agent" minOccurs="0" maxOccurs="unbounded"/>						<xs:element name="riskTreatmentStrategy" type="xs:string"/>						<xs:element name="riskTreatment" type="xs:string"/>						<xs:element name="log" minOccurs="0" maxOccurs="unbounded">							<xs:complexType>								<xs:sequence>									<xs:element ref="erms:dates"/>									<xs:element name="logPlan" type="xs:string"/>									<xs:element name="remainingLikelihood" type="xs:string"/>									<xs:element name="remainingRiskRating" type="xs:string"/>									<xs:element name="remainingImpact" type="xs:string"/>								</xs:sequence>							</xs:complexType>						</xs:element>					</xs:sequence>				</xs:complexType>			</xs:element>		</xs:sequence>	</xs:complexType>		<xs:complexType name="geographyType">		<xs:annotation>			<xs:documentation>Describes the geografical perimeters</xs:documentation>		</xs:annotation>			<xs:sequence>				<xs:element name="name" type="xs:string"/>			</xs:sequence>			<xs:attribute name="locationType" use="required">				<xs:annotation>					<xs:documentation>Requiered typing of the location. When set to the value "other" an own extending value can be given with the attribute "otherLocationType"</xs:documentation>				</xs:annotation>				<xs:simpleType>					<xs:restriction base="xs:string">						<xs:enumeration value="country"/>						<xs:enumeration value="city"/>						<xs:enumeration value="region"/>						<xs:enumeration value="other"/>					</xs:restriction>				</xs:simpleType>			</xs:attribute>			<xs:attribute name="otherLocationType" type="xs:string" use="optional">				<xs:annotation>					<xs:documentation>When attribe "locationType" is set to "other" this attribute is used to describe the locationType</xs:documentation>				</xs:annotation>			</xs:attribute>	</xs:complexType>		<xs:complexType name="monitoringsType">		<xs:sequence>			<xs:element name="monitoring" maxOccurs="unbounded">				<xs:complexType>					<xs:sequence>						<xs:element ref="erms:extraId"/>						<xs:element name="monitoringTitle" type="xs:string"/>						<xs:element ref="erms:dates" minOccurs="0"/>						<xs:element name="financing" type="xs:string" minOccurs="0"/>						<xs:element name="monitoringDescription" type="xs:string" minOccurs="0"/>						<xs:element ref="erms:agent" minOccurs="0" maxOccurs="unbounded"/>					</xs:sequence>					<xs:attribute name="typeOfMonitoring" type="xs:string"/>				</xs:complexType>			</xs:element>		</xs:sequence>	</xs:complexType>		<xs:complexType name="partnerDataType">		<xs:sequence>			<xs:element name="year" type="xs:string"/>			<xs:element name="partner" type="xs:string"/>			<xs:element name="amountPaidByCos" type="xs:string"/>			<xs:element name="partnerGeography" type="geographyType"/>			<xs:element name="fin" type="xs:string"/>			<xs:element name="assessment" type="xs:dateTime"/>			<xs:element name="receivedAuditReport" type="xs:string"/>			<xs:element name="auditorFirm" type="xs:string"/>			<xs:element name="unqualifiedAuditorRep" type="xs:string"/>			<xs:element name="projectAssessment" type="xs:string"/>		</xs:sequence>	</xs:complexType>	</xs:schema>